<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.sweet.system.mapper.DictTypeMapper">    <!-- 通用查询映射结果 -->    <resultMap id="BaseResultMap" type="com.sweet.system.entity.DictType">        <id column="DICT_TYPE_ID" property="dictTypeId" />        <result column="CODE" property="code" />        <result column="NAME" property="name" />        <result column="DESCRIPTION" property="description" />        <result column="ORDER_NUM" property="orderNum" />        <result column="CREATE_TIME" property="createTime" />    </resultMap>    <!-- 通用查询结果列 -->    <sql id="Base_Column_List">        DICT_TYPE_ID, CODE, NAME, DESCRIPTION, ORDER_NUM, CREATE_TIME    </sql>    <select id="customPageList" resultType="com.sweet.system.entity.DictType"            parameterType="com.sweet.system.entity.DictType">        select        <include refid="Base_Column_List"/>        from t_dict_type where 1 = 1       <if test="param.name != null and param.name != ''">        and NAME like concat('%',#{param.name},'%')       </if>        order by ORDER_NUM asc    </select></mapper>