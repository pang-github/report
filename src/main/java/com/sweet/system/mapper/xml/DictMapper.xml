<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.sweet.system.mapper.DictMapper">    <!-- 通用查询映射结果 -->    <resultMap id="BaseResultMap" type="com.sweet.system.entity.Dict">        <id column="DICT_ID" property="dictId" />        <result column="DICT_TYPE_ID" property="dictTypeId" />        <result column="CODE" property="code" />        <result column="NAME" property="name" />        <result column="CREATE_TIME" property="createTime" />    </resultMap>    <!-- 通用查询结果列 -->    <sql id="Base_Column_List">        DICT_ID, DICT_TYPE_ID, CODE, NAME, CREATE_TIME    </sql>    <select id="customPageList" resultType="com.sweet.system.entity.Dict"            parameterType="com.sweet.system.entity.Dict">        select        <include refid="Base_Column_List"/>        from t_dict where 1 = 1       <if test="param.dictTypeId != null and param.dictTypeId != ''">        and DICT_TYPE_ID like concat('%',#{param.dictTypeId},'%')       </if>       <if test="param.code != null and param.code != ''">        and CODE like concat('%',#{param.code},'%')       </if>        order by CODE    asc    </select></mapper>