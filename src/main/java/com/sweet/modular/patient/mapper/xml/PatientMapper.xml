<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.sweet.modular.patient.mapper.PatientMapper">    <!-- 通用查询映射结果 -->    <resultMap id="BaseResultMap" type="com.sweet.modular.patient.entity.Patient">    <result column="id" property="id" />        <result column="userid" property="userid" />        <result column="patient_name" property="patientName" />        <result column="patient_sex" property="patientSex" />        <result column="patient_age" property="patientAge" />        <result column="patient_profession" property="patientProfession" />        <result column="patient_origo" property="patientOrigo" />        <result column="patient_nation" property="patientNation" />        <result column="phone_number" property="phoneNumber" />        <result column="patient_source" property="patientSource" />        <result column="patient_nickname" property="patientNickname" />        <result column="work_unit" property="workUnit" />        <result column="address" property="address" />        <result column="admission_time" property="admissionTime" />        <result column="blcjz" property="blcjz" />        <result column="marriage" property="marriage" />        <result column="create_time" property="createTime" />        <result column="patient_type" property="patientType" />    </resultMap>    <!-- 通用查询结果列 -->    <sql id="Base_Column_List">        id,        userid, patient_name, patient_sex, patient_age, patient_profession, patient_origo, patient_nation, phone_number, patient_source, work_unit, address, admission_time, blcjz, marriage, create_time, patient_type,patient_nickname    </sql>    <select id="customPageList" resultType="com.sweet.modular.patient.entity.Patient"            parameterType="com.sweet.modular.patient.entity.Patient">        select        <include refid="Base_Column_List"/>        from business_patient where 1 = 1       <if test="param.patientName != null and param.patientName != ''">        and patient_name like concat('%',#{param.patientName},'%')       </if>    </select></mapper>