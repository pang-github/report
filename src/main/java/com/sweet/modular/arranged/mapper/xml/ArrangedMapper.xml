<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.sweet.modular.arranged.mapper.ArrangedMapper">    <!-- 通用查询映射结果 -->    <resultMap id="BaseResultMap" type="com.sweet.modular.arranged.entity.Arranged">        <result column="id" property="id" />        <result column="patient_id" property="patientId" />        <result column="user_id" property="userId" />        <result column="patient_name" property="patientName" />        <result column="user_name" property="userName" />        <result column="arranged_site" property="arrangedSite" />        <result column="arranged_cont" property="arrangedCont" />        <result column="arranged_number" property="arrangedNumber" />        <result column="arranged_time" property="arrangedTime" />        <result column="create_time" property="createTime" />    </resultMap>    <!-- 通用查询结果列 -->    <sql id="Base_Column_List">        id, patient_id, user_id, patient_name, user_name, arranged_site, arranged_cont, arranged_number, arranged_time, create_time    </sql>    <select id="customPageList" resultType="com.sweet.modular.arranged.entity.Arranged"            parameterType="com.sweet.modular.arranged.entity.Arranged">        select        <include refid="Base_Column_List"/>        from business_arranged where 1 = 1       <if test="param.patientName != null and param.patientName != ''">        and patient_name like concat('%',#{param.patientName},'%')       </if>    </select></mapper>