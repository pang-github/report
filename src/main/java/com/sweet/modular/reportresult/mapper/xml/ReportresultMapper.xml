<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.sweet.modular.reportresult.mapper.ReportresultMapper">    <!-- 通用查询映射结果 -->    <resultMap id="BaseResultMap" type="com.sweet.modular.reportresult.entity.Reportresult">        <result column="id" property="id" />        <result column="TJH000" property="tjh000" />        <result column="YDZWS0" property="ydzws0" />        <result column="YDSFWC" property="ydsfwc" />        <result column="ZSZWS0" property="zszws0" />        <result column="ZSSFWC" property="zssfwc" />        <result column="JCCS00" property="jccs00" />        <result column="CGCS00" property="cgcs00" />        <result column="CGL000" property="cgl000" />        <result column="SCRQ00" property="scrq00" />        <result column="SCSJ00" property="scsj00" />        <result column="QT0001" property="qt0001" />        <result column="QT0002" property="qt0002" />        <result column="QT0003" property="qt0003" />        <result column="BZ0000" property="bz0000" />    </resultMap>    <!-- 通用查询结果列 -->    <sql id="Base_Column_List">        id, TJH000, YDZWS0, YDSFWC, ZSZWS0, ZSSFWC, JCCS00, CGCS00, CGL000, SCRQ00, SCSJ00, QT0001, QT0002, QT0003, BZ0000    </sql>    <select id="customPageList" resultType="com.sweet.modular.reportresult.entity.Reportresult"            parameterType="com.sweet.modular.reportresult.entity.Reportresult">        select        <include refid="Base_Column_List"/>        from reportresult where 1 = 1       <if test="param.tjh000 != null and param.tjh000 != ''">        and TJH000 like concat('%',#{param.tjh000},'%')       </if>    </select></mapper>