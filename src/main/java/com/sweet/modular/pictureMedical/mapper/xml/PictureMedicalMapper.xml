<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.sweet.modular.pictureMedical.mapper.PictureMedicalMapper">    <!-- 通用查询映射结果 -->    <resultMap id="BaseResultMap" type="com.sweet.modular.pictureMedical.entity.PictureMedical">    <result column="id" property="id" />        <result column="check_doctor" property="checkDoctor" />        <result column="patient_id" property="patientId" />        <result column="drug_type" property="drugType" />        <result column="picture" property="picture" />        <result column="DRUG_NUMBER" property="drugNumber" />        <result column="DESCRIPTION" property="description" />        <result column="CREATE_TIME" property="createTime" />    </resultMap>    <!-- 通用查询结果列 -->    <sql id="Base_Column_List">        id,        check_doctor, patient_id, drug_type, picture, DRUG_NUMBER, DESCRIPTION, CREATE_TIME    </sql>    <select id="customPageList" resultType="com.sweet.modular.pictureMedical.entity.PictureMedical"            parameterType="com.sweet.modular.pictureMedical.entity.PictureMedical">        select        <include refid="Base_Column_List"/>        from business_picture_medical where 1 = 1       <if test="param.checkDoctor != null and param.checkDoctor != ''">        and check_doctor like concat('%',#{param.checkDoctor},'%')       </if>       <if test="param.drugType != null and param.drugType != ''">        and drug_type like concat('%',#{param.drugType},'%')       </if>       <if test="param.createTime != null and param.createTime != ''">        and CREATE_TIME like concat('%',#{param.createTime},'%')       </if>    </select></mapper>