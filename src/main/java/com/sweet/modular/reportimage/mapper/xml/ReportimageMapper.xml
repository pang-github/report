<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.sweet.modular.reportimage.mapper.ReportimageMapper">    <!-- 通用查询映射结果 -->    <resultMap id="BaseResultMap" type="com.sweet.modular.reportimage.entity.Reportimage">        <result column="id" property="id" />        <result column="TJH000" property="tjh000" />        <result column="CTRQ00" property="ctrq00" />        <result column="TPBT00" property="tpbt00" />        <result column="BDY000" property="bdy000" />        <result column="JCBG00" property="jcbg00" jdbcType="BLOB" />    </resultMap>    <!-- 通用查询结果列 -->    <sql id="Base_Column_List">        id, TJH000, CTRQ00, TPBT00, BDY000, JCBG00    </sql>    <select id="customPageList" resultType="com.sweet.modular.reportimage.entity.Reportimage"            parameterType="com.sweet.modular.reportimage.entity.Reportimage">        select        <include refid="Base_Column_List"/>        from reportimage where 1 = 1       <if test="param.tjh000 != null and param.tjh000 != ''">        and TJH000 like concat('%',#{param.tjh000},'%')       </if>    </select></mapper>