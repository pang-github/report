<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.sweet.modular.rxdrug.mapper.RxdrugMapper">    <!-- 通用查询映射结果 -->    <resultMap id="BaseResultMap" type="com.sweet.modular.rxdrug.entity.Rxdrug">        <id column="ID" property="id" />        <result column="RECORD_ID" property="recordId" />        <result column="DRUG_NAME" property="drugName" />        <result column="DRUG_NUMBER" property="drugNumber" />        <result column="DRUG_UNIT" property="drugUnit" />        <result column="DRUG_PRICE" property="drugPrice" />    </resultMap>    <!-- 通用查询结果列 -->    <sql id="Base_Column_List">        ID, RECORD_ID, DRUG_NAME, DRUG_NUMBER, DRUG_UNIT, DRUG_PRICE    </sql>    <select id="customPageList" resultType="com.sweet.modular.rxdrug.entity.Rxdrug"            parameterType="com.sweet.modular.rxdrug.entity.Rxdrug">        select        <include refid="Base_Column_List"/>        from business_rxdrug where 1 = 1       <if test="param.recordId != null and param.recordId != ''">        and RECORD_ID like concat('%',#{param.recordId},'%')       </if>       <if test="param.drugName != null and param.drugName != ''">        and DRUG_NAME like concat('%',#{param.drugName},'%')       </if>       <if test="param.id != null and param.id != ''">        and ID like concat('%',#{param.id},'%')       </if>    </select></mapper>