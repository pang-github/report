<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.sweet.modular.clinicPharmacy.mapper.ClinicPharmacyMapper">    <!-- 通用查询映射结果 -->    <resultMap id="BaseResultMap" type="com.sweet.modular.clinicPharmacy.entity.ClinicPharmacy">        <id column="id" property="id" />        <result column="record_id" property="recordId" />        <result column="user_id" property="userId" />        <result column="create_time" property="createTime" />        <result column="interval_time" property="intervalTime" />        <result column="state_type" property="stateType" />    </resultMap>    <!-- 通用查询结果列 -->    <sql id="Base_Column_List">        id, record_id, user_id, create_time, interval_time, state_type    </sql>    <select id="customPageList" resultType="com.sweet.modular.clinicPharmacy.entity.ClinicPharmacy"            parameterType="com.sweet.modular.clinicPharmacy.entity.ClinicPharmacy">        select        <include refid="Base_Column_List"/>        from business_clinic_pharmacy where 1 = 1       <if test="param.id != null and param.id != ''">        and id like concat('%',#{param.id},'%')       </if>       <if test="param.recordId != null and param.recordId != ''">        and record_id like concat('%',#{param.recordId},'%')       </if>       <if test="param.userId != null and param.userId != ''">        and user_id like concat('%',#{param.userId},'%')       </if>       <if test="param.createTime != null and param.createTime != ''">        and create_time like concat('%',#{param.createTime},'%')       </if>       <if test="param.intervalTime != null and param.intervalTime != ''">        and interval_time like concat('%',#{param.intervalTime},'%')       </if>       <if test="param.stateType != null and param.stateType != ''">        and state_type like concat('%',#{param.stateType},'%')       </if>    </select></mapper>