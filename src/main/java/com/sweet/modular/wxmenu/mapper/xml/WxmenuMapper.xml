<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.sweet.modular.wxmenu.mapper.WxmenuMapper">    <!-- 通用查询映射结果 -->    <resultMap id="BaseResultMap" type="com.sweet.modular.wxmenu.entity.Wxmenu">        <id column="BUTTON_ID" property="buttonId" />        <result column="PARENT_ID" property="parentId" />        <result column="BUTTON_INDEX" property="buttonIndex" />        <result column="TYPE" property="type" />        <result column="NAME" property="name" />        <result column="URL" property="url" />        <result column="MEDIA_ID" property="mediaId" />        <result column="MATERIAL_URL" property="materialUrl" />        <result column="MATERIAL_TITLE" property="materialTitle" />        <result column="CONTENT" property="content" />        <result column="PAGEPATH" property="pagepath" />        <result column="APPID" property="appid" />        <result column="KEY" property="key" />        <result column="TIER" property="tier" />        <result column="CREATE_TIME" property="createTime" />    </resultMap>    <!-- 通用查询结果列 -->    <sql id="Base_Column_List">        BUTTON_ID, PARENT_ID, BUTTON_INDEX, TYPE, NAME, URL, MEDIA_ID,PAGEPATH,APPID,METERIAL_URL,METERIAL_TITLE,CONTENT,KEY,TIER, CREATE_TIME    </sql>    <select id="customPageList" resultType="com.sweet.modular.wxmenu.entity.Wxmenu"            parameterType="com.sweet.modular.wxmenu.entity.Wxmenu">        select        <include refid="Base_Column_List"/>        from business_wxmenu where 1 = 1       <if test="param.parentId != null and param.parentId != ''">        and PARENT_ID like concat('%',#{param.parentId},'%')       </if>    </select></mapper>