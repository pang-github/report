<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.sweet.modular.drug.mapper.DrugMapper">    <!-- 通用查询映射结果 -->    <resultMap id="BaseResultMap" type="com.sweet.modular.drug.entity.Drug">    <result column="id" property="id" />        <result column="patient_id" property="patientId" />        <result column="drug_num" property="drugNum" />        <result column="drug_name" property="drugName" />        <result column="drug_unit" property="drugUnit" />        <result column="manufacturer" property="manufacturer" />        <result column="drug_price" property="drugPrice" />        <result column="drug_explain" property="drugExplain" />        <result column="upprice" property="upprice" />        <result column="CREATE_TIME" property="createTime" />    </resultMap>    <!-- 通用查询结果列 -->    <sql id="Base_Column_List">        id,        patient_id, drug_num, drug_name, drug_unit, manufacturer, drug_price, drug_explain, upprice,CREATE_TIME    </sql>    <select id="customPageList" resultType="com.sweet.modular.drug.entity.Drug"            parameterType="com.sweet.modular.drug.entity.Drug">        select        <include refid="Base_Column_List"/>        from business_drug where 1 = 1       <if test="param.drugName != null and param.drugName != ''">        and drug_name like concat('%',#{param.drugName},'%')       </if>       order by create_time desc    </select></mapper>