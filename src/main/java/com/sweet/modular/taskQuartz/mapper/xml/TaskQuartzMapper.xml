<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.sweet.modular.taskQuartz.mapper.TaskQuartzMapper">    <!-- 通用查询映射结果 -->    <resultMap id="BaseResultMap" type="com.sweet.modular.taskQuartz.entity.TaskQuartz">    <result column="id" property="id" />        <result column="job_group" property="jobGroup" />        <result column="job_name" property="jobName" />        <result column="description" property="description" />        <result column="cron_expression" property="cronExpression" />        <result column="job_class_name" property="jobClassName" />        <result column="job_status" property="jobStatus" />        <result column="create_id" property="createId" />        <result column="create_time" property="createTime" />        <result column="modify_time" property="modifyTime" />        <result column="tpl_id" property="tplId" />        <result column="tpl_name" property="tplName" />        <result column="patient_type" property="patientType" />    </resultMap>    <!-- 通用查询结果列 -->    <sql id="Base_Column_List">        id,        job_group, job_name, description, cron_expression, job_class_name, job_status, create_id, create_time, modify_time, tpl_id, tpl_name, patient_type    </sql>    <select id="customPageList" resultType="com.sweet.modular.taskQuartz.entity.TaskQuartz"            parameterType="com.sweet.modular.taskQuartz.entity.TaskQuartz">        select        <include refid="Base_Column_List"/>        from task_quartz where 1 = 1       <if test="param.jobName != null and param.jobName != ''">        and job_name like concat('%',#{param.jobName},'%')       </if>    </select></mapper>