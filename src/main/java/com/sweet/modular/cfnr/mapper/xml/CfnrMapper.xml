<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.sweet.modular.cfnr.mapper.CfnrMapper">    <!-- 通用查询映射结果 -->    <resultMap id="BaseResultMap" type="com.sweet.modular.cfnr.entity.Cfnr">    <result column="id" property="id" />        <result column="recipe_id" property="recipeId" />        <result column="drug_id" property="drugId" />        <result column="drug_name" property="drugName" />        <result column="drug_mass" property="drugMass" />        <result column="drug_unit" property="drugUnit" />        <result column="drug_price" property="drugPrice" />    </resultMap>    <!-- 通用查询结果列 -->    <sql id="Base_Column_List">        id,        recipe_id, drug_id, drug_name, drug_mass, drug_unit, drug_price    </sql>    <select id="customPageList" resultType="com.sweet.modular.cfnr.entity.Cfnr"            parameterType="com.sweet.modular.cfnr.entity.Cfnr">        select        <include refid="Base_Column_List"/>        from business_cfnr where 1 = 1       <if test="param.drugName != null and param.drugName != ''">        and drug_name like concat('%',#{param.drugName},'%')       </if>       <if test="param.recipeId != null and param.recipeId != ''">        and recipe_id like concat('%',#{param.recipeId},'%')       </if>    </select></mapper>