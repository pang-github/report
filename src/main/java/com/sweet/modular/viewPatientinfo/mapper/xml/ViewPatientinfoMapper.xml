<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.sweet.modular.viewPatientinfo.mapper.ViewPatientinfoMapper">    <!-- 通用查询映射结果 -->    <resultMap id="BaseResultMap" type="com.sweet.modular.viewPatientinfo.entity.ViewPatientinfo">        <result column="id" property="id" />        <result column="TJH000" property="tjh000" />        <result column="XM0000" property="xm0000" />        <result column="CSRQGS" property="csrqgs" />        <result column="XB0000" property="xb0000" />        <result column="SG0000" property="sg0000" />        <result column="TZ0000" property="tz0000" />        <result column="DH0000" property="dh0000" />        <result column="SJKE00" property="sjke00" />        <result column="SJYS00" property="sjys00" />        <result column="BZ0000" property="bz0000" />    </resultMap>    <!-- 通用查询结果列 -->    <sql id="Base_Column_List">        id, TJH000, XM0000, CSRQGS, XB0000, SG0000, TZ0000, DH0000, SJKE00, SJYS00, BZ0000    </sql>    <select id="customPageList" resultType="com.sweet.modular.viewPatientinfo.entity.ViewPatientinfo"            parameterType="com.sweet.modular.viewPatientinfo.entity.ViewPatientinfo">        select        <include refid="Base_Column_List"/>        from view_patientinfo where 1 = 1       <if test="param.tjh000 != null and param.tjh000 != ''">        and TJH000 like concat('%',#{param.tjh000},'%')       </if>    </select></mapper>