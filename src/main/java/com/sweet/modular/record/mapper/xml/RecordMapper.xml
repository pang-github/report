<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.sweet.modular.record.mapper.RecordMapper">    <!-- 通用查询映射结果 -->    <resultMap id="BaseResultMap" type="com.sweet.modular.record.entity.Record">    <result column="id" property="id" />        <result column="check_time" property="checkTime" />        <result column="check_doctor" property="checkDoctor" />        <result column="patient_name" property="patientName" />        <result column="patient_id" property="patientId" />        <result column="examine" property="examine" />        <result column="diagnose" property="diagnose" />        <result column="recipe" property="recipe" />        <result column="drug_money" property="drugMoney" />        <result column="picture" property="picture" />        <result column="zycs" property="zycs" />        <result column="state_type" property="stateType" />    </resultMap>    <!-- 通用查询结果列 -->    <sql id="Base_Column_List">        id,        check_time, patient_name, check_doctor, patient_id, examine, diagnose, recipe, drug_money, picture, zycs,state_type    </sql>    <select id="customPageList" resultType="com.sweet.modular.record.entity.Record"            parameterType="com.sweet.modular.record.entity.Record">        select        <include refid="Base_Column_List"/>        from business_record where 1 = 1       <if test="param.id != null and param.id != ''">        and id like concat('%',#{param.id},'%')       </if>       <if test="param.patientName != null and param.patientName != ''">        and patient_name like concat('%',#{param.patientName},'%')       </if>       <if test="param.checkTime != null and param.checkTime != ''">        and check_time like concat('%',#{param.checkTime},'%')       </if>       <if test="param.patientId != null and param.patientId != ''">        and patient_id like concat('%',#{param.patientId},'%')       </if>        order by check_time desc    </select></mapper>