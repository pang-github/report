package ${package.Controller};<% var tableId = cfg.fieldList[0].columnName; %>import com.sweet.core.translationDict.DictParam;import com.sweet.core.translationDict.TranslationDict;import org.springframework.web.bind.annotation.RequestMapping;import com.sweet.core.model.ResultBean;import com.sweet.core.model.system.LayuiPageInfo;import com.sweet.modular.${package.ModuleName}.entity.${table.entityName};import com.sweet.modular.${package.ModuleName}.service.${table.entityName}Service;import org.springframework.beans.factory.annotation.Autowired;import org.springframework.web.bind.annotation.RequestMapping;import com.sweet.core.util.StringUtil;import org.springframework.stereotype.Controller;import org.springframework.web.bind.annotation.ResponseBody;<% if(restControllerStyle){ %>import org.springframework.web.bind.annotation.RestController;<% }else{ %>import org.springframework.stereotype.Controller;<% } %><% if(isNotEmpty(superControllerClassPackage)){ %>import ${superControllerClassPackage};<% } %>/** * <p> * ${table.comment!} 前端控制器 * </p> * * @author ${author} * @since ${date} */<% if(restControllerStyle){ %>@RestController<% }else{ %>@Controller<% } %>@RequestMapping("<% if(isNotEmpty(cfg.lastModularName)){ %>/${cfg.lastModularName}<% } %><% if(isNotEmpty(package.ModuleName)){ %>/${package.ModuleName}<% } %>")<% if(kotlin){ %>class ${table.controllerName}<% if(isNotEmpty(superControllerClass)){ %> : ${superControllerClass}()<% } %><% }else{ %>    <% if(isNotEmpty(superControllerClass)){ %>public class ${table.controllerName} extends ${superControllerClass} {    <% }else{ %>public class ${table.controllerName} {    <% } %>    @Autowired    ${table.entityName}Service ${package.ModuleName}Service;    /**     * 列表页     */    @RequestMapping("/${package.ModuleName}_list")    public String list(){        return "/modular/${package.ModuleName}/${package.ModuleName}_list";    }    /**     * 编辑页     */    @RequestMapping("/${package.ModuleName}_edit")    public String edit(){        return "/modular/${package.ModuleName}/${package.ModuleName}_edit";    }    /**     * 添加/编辑     */    @RequestMapping("/edit${table.entityName}")    @ResponseBody    public ResultBean edit${table.entityName}(${table.entityName} ${package.ModuleName}){        ${package.ModuleName}Service.saveOrUpdate(${package.ModuleName});        return ResultBean.success(${package.ModuleName});    }    /**     * 删除     */    @RequestMapping("/del${table.entityName}")    @ResponseBody    public ResultBean del${table.entityName}(${table.entityName} ${package.ModuleName}){        ${package.ModuleName}Service.removeById(${package.ModuleName});        return ResultBean.success(${package.ModuleName});    }    /**     * 添加修改菜单     * @param menu     * @return     */    @RequestMapping("/get${table.entityName}Detail")    @ResponseBody    public ResultBean get${table.entityName}Detail(String ${tableId}){        ${table.entityName} ${package.ModuleName} = ${package.ModuleName}Service.getById(${tableId});        return ResultBean.success(${package.ModuleName});    }    /**     * 列表数据     */    @RequestMapping("/get${table.entityName}List")    @ResponseBody    @TranslationDict({ <% for(item in cfg.fieldList){ %>   <% if(item.type=='select'){ %>            @DictParam(dictType = "${item.dic}",dictValueFiled = "${item.columnName}"),   <% } %>  <% } %>    })    public LayuiPageInfo get${table.entityName}List(${table.entityName} ${table.entityName}){        LayuiPageInfo pageInfo = ${package.ModuleName}Service.findPageBySpec(${table.entityName});        return pageInfo;    }}<% } %>